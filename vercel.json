{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/.*", "dest": "/api/index.js" },
      { "src": "/(.*)", "dest": "/app.js" }
    ]
  }

  